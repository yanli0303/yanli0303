{
  "name": "github-profile-readme-yanli0303",
  "version": "1.0.0",
  "description": "Yan Li's GitHub profile readme.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "postinstall": "([ -d .git ] && ln -s -f -v $(pwd)/pre-commit.sh .git/hooks/pre-commit) || echo 'No .git directory, is this a git worktree?'",
    "lint": "biome check --write src",
    "build": "tsx src/index.ts"
  },
  "dependencies": {
    "node-build-tools": "^0.3.2",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanli0303/yanli0303.git"
  },
  "keywords": [
    "GitHub",
    "README",
    "yanli0303"
  ],
  "author": "Yan Li",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/yanli0303/yanli0303/issues"
  },
  "homepage": "https://github.com/yanli0303/yanli0303#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.2.7",
    "@types/node": "^24.9.1",
    "lint-staged": "^16.2.6",
    "tsx": "^4.20.6"
  },
  "lint-staged": {
    "*.{mjs,cjs,js,jsx,ts,tsx}": [
      "biome check --write"
    ]
  }
}
